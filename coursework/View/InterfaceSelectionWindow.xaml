<Window x:Class="CourseWork.View.InterfaceSelectionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CourseWork.View"
        mc:Ignorable="d"
        Title="InterfaceSelectionWindow" Height="186" Width="246">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Orientation="Vertical" HorizontalAlignment="Center"
                    VerticalAlignment="Center" 
                    Width="200">
            <Label Content="Выберете интерфейс"/>
            <ComboBox />
            
        </StackPanel>
        <Button Grid.Row="1" Content="Выбрать" FontSize="20"/>
    </Grid>
</Window>
